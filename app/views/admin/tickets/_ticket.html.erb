<tr>
  <td><%= ticket.id %></td>
  <td><%= ticket.user.email %></td>
  <td><%= ticket.first_name + ' ' + ticket.last_name %></td>
  <td><%= link_to 'more_horiz', admin_ticket_path(ticket), class: 'material-icons button edit' %>
    <%= link_to 'edit', edit_admin_ticket_path(ticket), class: 'material-icons button edit' %>
    <%= link_to 'delete', admin_ticket_path(ticket), class: 'material-icons button delete', method: :delete, data: {confirm: t('common.confirm_del') } %></td>
</tr>